<!DOCTYPE html>
<html>
<body>

<div style="width:100%;height:0px;position:relative;padding-bottom:56.250%;" data={{data}}>
  <iframe src={{src}} 
    frameborder="0" width="1024" height="768" 
    allowfullscreen allow="autoplay" style="width:1024px;height:768px;position:absolute;left:0px;top:0px;overflow:hidden;"></iframe>
</div>

<div>
    <button id="btn">Remove</button>
</div>


<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js">
  $("#myIframe").load(function() {
    var $c = $('#myIframe').contents();
    $c.find(".ytp-show-cards-title").each(function (index, element) {
        $(this).remove()
        console.log("removed one linkUp");
    });
});
</script>

</body>
</html>